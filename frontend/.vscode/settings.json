{
  "files.exclude": {
    ".git/": true,
    "node_modules/": false,
    "public/": false
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/public": true
  },
  "editor.rulers": [
    120
  ],
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "eslint.autoFixOnSave": true,
  "eslint.validate": [
    {
      "language": "javascript",
      "autoFix": true
    },
    {
      "language": "javascriptreact",
      "autoFix": true
    }
  ],
  "debug.node.autoAttach": "on",
  "cSpell.words": [
    "browserslist",
    "devtools",
    "fontawesome",
    "fortawesome",
    "reactstrap",
    "testid"
  ]
}